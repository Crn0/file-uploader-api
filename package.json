{
  "name": "prisma-template",
  "private": true,
  "description": "",
  "version": "0.0.0",
  "main": "app.js",
  "type": "module",
  "prisma": {
    "schema": "./src/db/models/schema.prisma"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./src/bin/www.js",
    "prisma": "npx prisma init",
    "prismaDev": "dotenv -e .env.dev -- npx prisma generate",
    "prismaProd": "dotenv -e .env.prod -- npx prisma generate ",
    "dev": "dotenv -e .env.dev -- node --watch ./src/bin/www.js",
    "prod": "dotenv -e .env.prod -- node --watch ./src/bin/www.js",
    "server": "DEBUG=<directory-name>:* npm run dev",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "update": "ncu -u -x eslint",
    "format": "prettier --write \"**/*.{js,mjs}\""
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1"
  }
}
